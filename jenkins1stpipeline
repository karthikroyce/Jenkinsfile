pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Build Hello World'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploy runs Hello World'
            }
        }
        stage('Test') {
            steps {
                echo 'Test phase runs Hello World'
            }
        }
        stage('Release') {
            steps {
                echo 'Release phase runs Hello World'
            }
        }
    }
}
